
<h1> {{title}} </h1>

<div class="container">
    <span id="message"></span>
    <div class="card">
        <div class="card-body">
            <h1 class="mt-4 mb-4 text-center text-primary"><b>{{title}} </b></h1>
            <div class="table-responsive">
                <table class="table table-striped table-bordered" id="sample_data">
                    <thead>
                    <tr>
                        <th>id</th>
                        <th>ФИО</th>
                        <th> Итоговая оценка</th>
                    </tr>
                    </thead>

                    <tbody>{{# each students}}
                        <tr>
                            <td>{{this.id}}</td>
                            <td><a href="">{{this.student_fio}}</a></td>
                            <td>{{this.result_grade}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
