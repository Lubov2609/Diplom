
<h1> {{title}} </h1>

<div class="container">
    <span id="message"></span>
    <div class="card">
        <div class="card-body">
            <h1 class="mt-4 mb-4 text-center text-primary"><b>{{title}} </b></h1>
            <div class="table-responsive">
                <table class="table table-striped table-bordered" id="sample_data">
                    <thead>
                    <tr>
                        {{#each users}}
                        <th>{{this.user_fio}}</th>
                        {{/each}}
                    </tr>
                    </thead>

                    <tbody>
                        <tr>
                            <td></td>

                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
